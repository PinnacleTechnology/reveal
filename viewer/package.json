{
  "name": "@cognite/reveal",
  "version": "2.0.0-beta.3",
  "private": true,
  "description": "WebGL based 3D viewer for CAD and point clouds processed in Cognite Data Fusion.",
  "repository": {
    "type": "git",
    "directory": "viewer",
    "url": "https://github.com/cognitedata/reveal.git"
  },
  "homepage": "https://github.com/cognitedata/reveal/tree/master/viewer",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "yarn run clean && webpack --env.development",
    "build:prod": "yarn run clean && webpack --env.production",
    "build:watch": "yarn run clean && webpack --env.development --watch",
    "serve": "cd ../examples && yarn && yarn start",
    "mkdist": "shx mkdir -p dist && shx cp package.json README.md LICENSE dist",
    "clean": "shx rm -rf dist/ && yarn run mkdist",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js --max-warnings 0 && yarn run tsc",
    "lint:fix": "eslint . --ext .ts,.js --max-warnings 0 --fix",
    "prebump": "yarn version --no-git-tag-version && yarn",
    "bump": " yarn bump:commit && yarn bump:tag && yarn bump:push",
    "bump:commit": "git add -A && cross-var git commit -m \"chore(viewer): bump version $npm_package_version\"",
    "bump:tag": "cross-var git tag $npm_package_name@$npm_package_version",
    "bump:push": "git push -u origin head && git push --tags",
    "release": "yarn build:prod && cd dist && yarn publish --non-interactive"
  },
  "workspaces": [
    "core"
  ],
  "devDependencies": {
    "@cognite/sdk": "^3.4.0",
    "@cognite/sdk-core": "^1.0.0",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "raw-loader": "^4.0.2",
    "shx": "^0.3.3",
    "ts-loader": "8.0.7",
    "typescript": "^4.3.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-log": "^3.0.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@cognite/potree-core": "1.2.1",
    "@cognite/reveal-parser-worker": "1.1.1",
    "@tweenjs/tween.js": "^18.6.4",
    "@types/three": "0.128.0",
    "assert": "^2.0.0",
    "comlink": "4.3.1",
    "glslify": "^7.1.1",
    "glslify-import": "^3.1.0",
    "lodash": "^4.17.20",
    "mixpanel-browser": "^2.39.0",
    "rxjs": "^7.1.0",
    "skmeans": "^0.11.3",
    "three": "0.128.0"
  },
  "peerDependencies": {
    "@cognite/sdk": "^3.0.0",
    "@cognite/sdk-core": "^1.0.0"
  },
  "author": "Lars Moastuen <lars.moastuen@cognite.com>",
  "contributors": [
    "Joachim Bj√∏rne <joachim.bjorne@cognite.com>",
    "Christopher Tannum <christopher.tannum@cognite.com>",
    "Maksim Nesterenko <maksim.nesterenko@cognite.com>"
  ],
  "license": "Apache-2.0",
  "sideEffects": false,
  "glslify": {
    "transform": [
      "glslify-import"
    ]
  }
}